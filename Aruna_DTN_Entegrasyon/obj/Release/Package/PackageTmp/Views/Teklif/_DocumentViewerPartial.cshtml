@Html.DevExpress().DocumentViewer(settings => {
    settings.Name = "DocumentViewer";
    settings.CallbackRouteValues = new { Controller = "Teklif", Action = "DocumentViewerPartial" };
    settings.ExportRouteValues = new { Controller = "Teklif", Action = "DocumentViewerPartialExport" };
    settings.Report = (XtraReport)Model;
    settings.ToolbarMode = DocumentViewerToolbarMode.Ribbon;
}).GetHtml()